    <class>br.com.velocity.sistema.entidades.CadCliente</class>
    <class>br.com.velocity.sistema.entidades.CadDocumentos</class>
    <class>br.com.velocity.sistema.entidades.CadFornecedor</class>
    <class>br.com.velocity.sistema.entidades.CadEmail</class>
    <class>br.com.velocity.sistema.entidades.CadEndereco</class>
    <class>br.com.velocity.sistema.entidades.CadFuncionarios</class>
    <class>br.com.velocity.sistema.entidades.CadGrupos</class>
